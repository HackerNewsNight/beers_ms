@{
    ViewBag.Title = "Beer List";
}
<h1>@ViewBag.Title</h1>
<div ng-controller="HomeController">
    <table class="table table-hover table-condensed">
        <thead>
            <tr>
                <th>Name</th>
                <th>Style</th>
                <th>Rating</th>
            </tr>
        </thead>
        <tbody ng-repeat="beer in beers">
            <tr>
                <td>{{beer.name}}</td>
                <td>{{beer.style}}</td>
                <td>{{beer.rating}}</td>
            </tr>
        </tbody>
    </table>
</div>